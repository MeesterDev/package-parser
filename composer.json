{
  "name": "meesterdev/package-parser",
  "description": "Parse composer/npm package locks into a list of packages with some basic information.",
  "minimum-stability": "stable",
  "license": "MIT",
  "version": "1.0.1",
  "authors": [
    {
      "name": "Thomas Meester",
      "homepage": "https://meester.dev/"
    }
  ],
  "homepage": "https://meester.dev/",
  "require": {
    "php": ">=8.1",
    "meesterdev/file-wrapper": "^0.0.2",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5|^10"
  },
  "autoload": {
    "psr-4": {
      "MeesterDev\\PackageParser\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MeesterDev\\Tests\\": "tests"
    }
  }
}
